
-------------------------------------------------------------------------------
-- compose(a, b) = x => b(a(x))

compose(a, b) = x => b(a(x))

-------------------------------------------------------------------------------
-- id(a) = a

id(a) = a

-------------------------------------------------------------------------------
-- const(a, b) = a
--
-- example: length(list) = list |> map(const(1)) |> sum

const(a, b) = a

-------------------------------------------------------------------------------
-- Get an infinte list [init, func(init), func(func(init)) ...]

iterate(func, init) = [init] ++ iterate(func, func(init))
